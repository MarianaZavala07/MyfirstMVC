@{
    ViewData["Title"] = "Editar Maestro";
}

<div class="text-center">
    <h1 class="display-2"> Editar Datos de Maestro</h1>
</div>



<form asp-controller="Teacher" asp-action="TeacherEdit">
<div class="mb-3">
        <label for="nombre" class="form-label">Nombre</label>
        <input type="text" class="form-control" id="nombre" name="nombre" placeholder="Escriba nombre de Maestro" />
    </div>
    <button type="submit" class="btn btn-primary">Guardar</button>
      <a asp-controller="Teacher" asp-action="ListOfTeachers"
       class="btn btn-secundary mb-3"> Regresar</a>

</form>